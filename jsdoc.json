{
  "source": {
    "include": ["backend", "frontend"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules/|docs/)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
}
