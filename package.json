{
  "name": "jimmy-yodeler",
  "version": "1.0.0",
  "description": "Military Voice Procedure Trainer",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "start-test": "node backend/server-test.js",
    "dev": "nodemon backend/server.js",
    "dev-test": "nodemon backend/server-test.js",
    "test": "jest",
    "create-db": "mysql -u root -p -e \"CREATE DATABASE IF NOT EXISTS jimmy_yodeler;\"",
    "setup-db": "mysql -u root -p jimmy_yodeler < database/schema.sql && mysql -u root -p jimmy_yodeler < database/seed.sql",
    "setup-db-full": "npm run create-db && npm run setup-db"
  },
  "keywords": [
    "military",
    "voice",
    "procedure",
    "training",
    "whisper",
    "tts"
  ],
  "author": "Jimmy Yodeler Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.1",
    "openai": "^4.12.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
